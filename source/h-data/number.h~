#ifndef __NUMBER_H__
#definde __NUMBER_H__
int number(struct number*)
struct number {
  uint8_t zero = 0xC0;
  uint8_t one = 0x86;
  uint8_t two = 0xA4;
  uint8_t three = 0xB0;
  uint8_t four = 0x99;
  uint8_t five = 0x92;
  uint8_t six = 0x82;
  uint8_t seven = 0xF8;
  uint8_t eight = 0x80;
  uint8_t nine = 0x90;
};

#endif
